{
  "name": "where-who-with-web",
  "version": "0.1.0",
  "scripts": {
    "start:dev": "webpack serve --config webpack.config.dev.js",
    "start:product": "webpack serve --config webpack.config.product.js",
    "build": "webpack --config webpack.config.product.js",
    "pack:update:check": "npx -p npm-check-updates -c 'ncu'",
    "pack:update:apply": "npx -p npm-check-updates -c 'ncu -u' && yarn",
    "pack:sort": "npx sort-package-json"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "copy-webpack-plugin": "^9.0.1",
    "html-minimizer-webpack-plugin": "^3.1.1",
    "ts-loader": "^9.2.3",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
